<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= contact.company %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.main_email %>
  </p>

  <p>
    <strong>Phone Numbers:</strong>
    <ul>
      <% contact.phone_numbers.each do |phone| %>
        <li><%= phone.label %>: <%= phone.number %></li>
      <% end %>
    </ul>
  </p>

  <p>
    <strong>Address:</strong>
    <%= contact.address %>
  </p>
</div>
