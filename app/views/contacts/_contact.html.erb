<div id="<%= dom_id contact %>">
  <p>
    <strong>First name:</strong>
    <%= contact.first_name %>
  </p>

  <p>
    <strong>Last name:</strong>
    <%= contact.last_name %>
  </p>

  <p>
    <strong>Company:</strong>
    <%= contact.company %>
  </p>

  <p>
    <strong>Main email:</strong>
    <%= contact.main_email %>
  </p>

  <p>
    <strong>Main phone:</strong>
    <%= contact.main_phone %>
  </p>

  <p>
    <strong>Address:</strong>
    <%= contact.address %>
  </p>

  <p>
    <strong>Latitude:</strong>
    <%= contact.latitude %>
  </p>

  <p>
    <strong>Longitude:</strong>
    <%= contact.longitude %>
  </p>

  <p>
    <strong>User:</strong>
    <%= contact.user_id %>
  </p>

</div>
